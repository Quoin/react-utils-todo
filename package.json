{
  "name": "@quoin/react-utils-todo",
  "version": "1.0.0",
  "description": "TODO list example with react-utils library",
  "scripts": {
    "check": "npm run eslint",
    "clean": "rimraf docs/",
    "mkdirs": "mkdirp docs/js docs/css docs/fonts",
    "precp": "npm run mkdirs",
    "eslint": "npm run eslint:node && npm run eslint:client && npm run eslint:client-test",
    "eslint:client": "eslint --fix --config=.eslintrc.client.json --ignore-pattern=\"*.test.js\" src",
    "eslint:client-test": "eslint --fix --config=.eslintrc.client.test.json --ignore-pattern=setup.test.js \"src/**/*.test.js\"",
    "eslint:node": "eslint --fix --config=.eslintrc.node.json *.js",
    "less": "lessc src/style.less docs/style.css",
    "postless": "uglifycss --output docs/style.min.css docs/style.css",
    "prebuild": "npm run clean && npm run check",
    "build": "webpack --env.prod",
    "postbuild": "npm run less",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack",
    "postbuild:dev": "npm run less",
    "test": "npm run test:unit",
    "test:unit": "mocha --require @babel/register --require src/setup.test.js \"src/**/*.unit.test.js?(x)\"",
    "coveralls": "cat reports/coverage.unit/lcov.info | coveralls",
    "coverage": "npm run coverage:unit",
    "coverage:unit": "nyc --report-dir=reports/coverage.unit npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quoin/react-utils-todo.git"
  },
  "keywords": [
    "Quoin",
    "React",
    "Utils",
    "TODO",
    "list"
  ],
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quoin/react-utils-todo/issues"
  },
  "homepage": "https://github.com/Quoin/react-utils-todo#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "@quoin/eslint-config-quoin": "^0.3.3",
    "@quoin/react-test-utils": "^1.0.0",
    "@quoin/react-utils": "^0.0.6",
    "@quoin/react-utils-bs3": "^1.0.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^3.4.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.1",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "immutable": "^3.8.2",
    "jquery": "^3.4.1",
    "jscpd": "^2.0.16",
    "less": "^3.10.3",
    "mkdirp": "^0.5.1",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "^0.33.1",
    "react-dom": "^16.12.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-concatenate-reducers": "^1.0.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.0",
    "uglifycss": "0.0.29",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
